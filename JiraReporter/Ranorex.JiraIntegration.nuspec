    <title>Ranorex Jira reporting modules</title>
    <version>0.1.2</version>
    <authors>Ranorex GmbH</authors>
    <owners>Ranorex GmbH</owners>
    <projectUrl>https://github.com/ranorex/Ranorex-Jira-Integration</projectUrl>
